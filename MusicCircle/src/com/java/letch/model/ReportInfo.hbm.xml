<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 用户举报信息表 -->
    <class name="com.java.letch.model.ReportInfo" table="reportinfo">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="uid" column="uid"/>
        <property name="uuid" column="uuid"/>
        <property name="uphone" column="uphone"/>
        <property name="unames" column="unames"/>
        <property name="beuid" column="beuid"/>
        <property name="beuuid" column="beuuid"/>
        <property name="beuphone" column="beuphone"/>
        <property name="beunames" column="beunames"/>
        <property name="reptypes" column="reptypes"/>
        <property name="repconte" column="repconte"/>
        <property name="reppices" column="reppices"/>
        <property name="addtimes" column="addtimes"/>
        <property name="replytie" column="replytie"/>
        <property name="updtimes" column="updtimes"/>
        <property name="stratues" column="stratues"/>
        <property name="remark" column="remark"/>
        
	        
    </class>
</hibernate-mapping>