<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 心动配置信息  -->
    <class name="com.java.letch.model.BeckoningInfo" table="beckoninginfo">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="beginnum" column="beginnum"/>
        <property name="endnumes" column="endnumes"/>
        <property name="beckurls" column="beckurls"/>
        <property name="contents" column="contents"/>
        <property name="remark" column="remark"/>
        
        <!-- 新版信息 -->
        
        
    </class>
</hibernate-mapping>