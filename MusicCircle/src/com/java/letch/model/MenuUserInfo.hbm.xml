<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 用户与后台菜单关联信息配置 -->
    <class name="com.java.letch.model.MenuUserInfo" table="menuuserinfo">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <!-- <property name="admid" column="admid"/>
        <property name="menid" column="menid"/> -->
        <property name="strtu" column="strtu"/>
        
        <many-to-one name="user" class="com.java.letch.model.AdminsUserInfo" column="admid" lazy="false" fetch="join"></many-to-one>
		<many-to-one name="menu" class="com.java.letch.model.MenuInfo" column="menid" lazy="false" fetch="join"></many-to-one>
		        
    </class>
</hibernate-mapping>