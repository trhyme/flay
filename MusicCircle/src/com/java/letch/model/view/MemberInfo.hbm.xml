<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 用户会员信息表 -->
    <class name="com.java.letch.model.MemberInfo" table="memberinfo">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="uid" column="uid"/>
        <property name="uuid" column="uuid"/>
        <property name="addtime" column="addtime"/>
        <property name="upetime" column="upetime"/>
        <property name="megid" column="megid"/>
        <property name="megname" column="megname"/>
        <property name="daysnum" column="daysnum"/>
        <property name="begintime" column="begintime"/>
        <property name="endestime" column="endestime"/>
        <property name="strutus" column="strutus"/>
        <property name="disabls" column="disabls"/>
        <property name="totalcost" column="totalcost"/>
        <property name="remark" column="remark"/>
        
    </class>
</hibernate-mapping>