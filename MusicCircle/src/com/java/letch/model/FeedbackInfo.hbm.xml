<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 用户反馈信息表 -->
    <class name="com.java.letch.model.FeedbackInfo" table="feedbackinfo">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="uid" column="uid"/>
        <property name="uuid" column="uuid"/>
        <property name="uphone" column="uphone"/>
        <property name="unames" column="unames"/>
        <property name="conten" column="conten"/>
        <property name="addtime" column="addtime"/>
        <property name="replyes" column="replyes"/>
        <property name="uptimes" column="uptimes"/>
        <property name="stratus" column="stratus"/>
        <property name="remarks" column="remarks"/>
        
    </class>
</hibernate-mapping>