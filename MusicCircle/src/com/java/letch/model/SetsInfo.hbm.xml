<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 信息设置表 -->
    <class name="com.java.letch.model.SetsInfo" table="sets">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="loginnb" column="loginnb"/>
        <property name="ratenb" column="ratenb"/>
        <property name="rewardnb" column="rewardnb"/>
        <property name="rechargearr" column="rechargearr"/>
        <property name="tapnb" column="tapnb"/>
        <property name="keywords" column="keywords"/>
        <property name="isopen" column="isopen"/>
        
        <!-- 新版信息 -->
        <property name="bluebrick" column="bluebrick"/> <!-- 蓝砖 -->
        <property name="redsbrick" column="redsbrick"/> <!-- 红砖 -->
        <property name="bluerates" column="bluerates"/> <!-- 蓝砖与红砖的联合比率 -->
        <property name="redsrates" column="redsrates"/> <!-- 蓝砖与红砖的联合比率 -->
        
    </class>
</hibernate-mapping>