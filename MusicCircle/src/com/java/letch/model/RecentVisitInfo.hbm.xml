<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 来访记录信息类  -->
    <class name="com.java.letch.model.RecentVisitInfo" table="recentvisitinfo">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="userid" column="userid"/>
        <property name="userseeid" column="userseeid"/>
        <property name="addtimes" column="addtimes"/>
        
        
    </class>
</hibernate-mapping>